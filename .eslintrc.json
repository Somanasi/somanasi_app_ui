{
    "env": {
        "browser": true,
        "jest": true,
        "es6": true
      },
    "plugins": ["import"],
    "extends": [
      "react-app",
      "eslint:recommended",
      "plugin:prettier/recommended",
      "prettier/react"
    ],
    "parserOptions": {
        "ecmaVersion": 2023,
        "sourceType": "module"
      },
    "rules": {
        "no-console": "warn",
        "no-eval": "error",
        "import/first": "error"
    }
  }
  